{
    "configurations": [
        {
            "preLaunchTask": "C Make",
            "name": "GCC (gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "${workspaceFolder}/NeuronSampler",
            "program":"${workspaceFolder}/NeuronSampler/NeuronSampler.exe",
            "MIMode": "gdb",
            "miDebuggerPath": "C:\\gcc\\mingw64\\bin\\gdb.exe",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            },
            {
                "description": "Set Disassembly Flavor to Intel",
                "text": "-gdb-set disassembly-flavor intel",
                "ignoreFailures": true
            }
        ]
    }
    ]
}